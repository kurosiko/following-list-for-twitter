{
	"manifest_version": 2,
	"name": "follow list",
	"version": "1.0",
    "background": {
        "scripts": [
            ["background.js"]
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "https://twitter.com/*/following"
            ],
            "js": ["main.js"]
            }
        ],
	"permissions": [
        "<all_urls>",
        "webRequest",
        "tabs"
    ]
}